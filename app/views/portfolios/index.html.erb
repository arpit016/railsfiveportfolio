<% @title = "Portfolios" %>
<div class="wrapper">
  <div class="page-header page-header-small" filter-color="orange">
    <div class="page-header-image" data-parallax="true" style="background-image: url('/assets/portfolio.jpg');">
    </div>
		<div class="container">
		  <div class="content-center">
		    <h3 class="wow fadeInDown title" data-wow-delay="1s">A Place for Entrepreneurs to Share and Discover New Stories</h3>
		  </div>
		</div>
  </div>


<div class="main main-raised">
  <div class="container" id="portfolio_top">
      <div class="row" id="portfolio_gallery">
          <%= render 'portfolios/portfolio', portfolio_items: @portfolio_items %>
      </div>    
  </div>
</div>
</div>
<%= render 'layouts/footer'%>